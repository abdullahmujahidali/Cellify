{
  "name": "cellify",
  "version": "0.1.0",
  "description": "A lightweight, affordable spreadsheet library for building Excel-like experiences",
  "type": "module",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/types/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "require": {
        "types": "./dist/types/index.d.ts",
        "default": "./dist/cjs/index.js"
      }
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "npm run build:esm && npm run build:cjs && npm run build:types && npm run build:cjs-package",
    "build:esm": "tsc -p tsconfig.esm.json",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:types": "tsc -p tsconfig.types.json",
    "build:cjs-package": "echo '{\"type\":\"commonjs\"}' > dist/cjs/package.json",
    "build:wasm": "cd wasm && ./build.sh",
    "build:all": "npm run build:wasm && npm run build",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "tsc --noEmit",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run clean && npm run build:all && npm run test",
    "demo": "vite demo",
    "demo:build": "vite build --config demo/vite.config.js"
  },
  "keywords": [
    "excel",
    "spreadsheet",
    "xlsx",
    "csv",
    "workbook",
    "cells",
    "grid"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@vitest/coverage-v8": "^4.0.15",
    "typescript": "^5.3.0",
    "vite": "^7.0.0",
    "vitest": "^4.0.15"
  },
  "dependencies": {
    "fflate": "^0.8.0"
  },
  "engines": {
    "node": "^20.19.0 || >=22.12.0"
  }
}
